{
    "name": "mypromo/connect-sdk",
    "description": "SDK for the mypromo connect API",
    "type": "sdk",
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "symfony/cache": "^5.4",
        "ext-json": "*",
        "symfony/validator": "^5.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyPromo\\Connect\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyPromo\\Connect\\SDK\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    }
}
